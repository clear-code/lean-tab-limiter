{
  "applications": {
    "gecko": {
      "id": "lean-tab-limiter-advanced@piro.sakura.ne.jp",
      "strict_min_version": "58.0a1"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },
  "description": "__MSG_extensionDescription__",
  "manifest_version": 2,
  "name": "Lean Tab Limiter Advanced",
  "author": "YUKI Hiroshi",
  "homepage_url": "https://github.com/piroor/lean-tab-limiter",
  "icons": {
    "48": "icons/lean-tab-limiter-48.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "default_locale": "en",
  "version": "0.1",
  "DEFAULT_TAB_LIMIT": 6,
  "DEFAULT_NOTIFY_BLOCKED": true,
  "DEFAULT_NOTIFY_BLCOKED_DURATION": 5000
}
